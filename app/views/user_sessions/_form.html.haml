= form_for @user_session do |f|
  - if @user_session.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_session.errors.count, "error")} prohibited this user_session from being saved:"
      %ul
        - @user_session.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :gamingsession_id
    = f.number_field :gamingsession_id
  .field
    = f.label :accepted
    = f.check_box :accepted
  .actions
    = f.submit 'Save'
